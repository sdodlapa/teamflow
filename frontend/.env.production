# ðŸš€ TeamFlow Frontend Production Environment Configuration

# Backend API Configuration
VITE_API_BASE_URL=https://teamflow-api-production.up.railway.app/api/v1
VITE_API_URL=https://teamflow-api-production.up.railway.app

# Application Configuration
VITE_APP_NAME=TeamFlow
VITE_APP_VERSION=2.0.0
VITE_ENVIRONMENT=production

# Feature Flags
VITE_ENABLE_WEBSOCKETS=true
VITE_ENABLE_FILE_UPLOAD=true
VITE_ENABLE_COLLABORATION=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_TEMPLATE_SYSTEM=true

# Performance & Monitoring
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_LOG_LEVEL=error

# Production Settings
VITE_DEBUG_MODE=false
VITE_API_TIMEOUT=15000

# Third-party Service Configuration (to be configured)
VITE_GOOGLE_CLIENT_ID=your-production-google-client-id
VITE_GITHUB_CLIENT_ID=your-production-github-client-id